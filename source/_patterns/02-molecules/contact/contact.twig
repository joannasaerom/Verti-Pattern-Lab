<div class="3u 6u$(medium) 12u$(small)">
  <section class="widget contact last">
    <h3>Contact Us</h3>
    <ul>
      {% for icon in contact.icons %}
        <li>
          <a href="{{icon.link}}" class="{{icon.class}}">
            <span class="label">
            {{icon.label}}
            </span>
          </a>
        </li>
      {% endfor %}
    </ul>
    <p>{{contact.address1}}</p>
    <p>{{contact.address2}}</p>
    <p>{{contact.phone}}</p>
  </section>
</div>
